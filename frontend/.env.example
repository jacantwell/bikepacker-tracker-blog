# Mapbox Token (required for map display)
VITE_PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# Photo Base URL (for S3 or local storage)
# For local development: /strava_data/media
# For production with S3: https://your-bucket.s3.amazonaws.com/strava-photos
VITE_STRAVA_PHOTOS_BASE_URL=/strava_data/media

# Journey start date (ISO format)
VITE_JOURNEY_START_DATE=2025-05-24T00:00:00Z

# API URL (if using external API)
VITE_API_URL=https://api.example.com

